.picker{
  padding: 26rpx;
  background-color: #FFFFFF;
}
.page-body-wrapper{
  display:flex;
  margin:40rpx 0;
}
.image {
  width: 210rpx;
  height: 210rpx;
}
.image-plus {
  width: 210rpx;
  height: 210rpx;
  border: 1px dotted #999;
  position: relative;
}
.image-plus-horizontal {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 4rpx;
  height: 80rpx;
  transform: translate(-50%, -50%);
}
.image-plus-vertical {
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #d9d9d9;
  width: 80rpx;
  height: 4rpx;
  transform: translate(-50%, -50%);
}
.page-body-info {
  margin:0 10rpx;
}
