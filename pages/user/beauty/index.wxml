<view class="personal_info bgf14">
  <view class="photo_wrap">
    <image src="{{userInfo.avatarUrl}}" class="photo"></image>
  </view>
  <view class="nickname font-16 colfff">
    {{userInfo.nickName}},会员分：0
  </view>
  <view class="font-14 text-center colfff">总积分：0</view>
</view>

<!-- article start -->

<view class="wode_item_wrap">
  <navigator wx:for="{{beautyList}}" url="{{item.url}}">
    <view class="wode_item">
      {{item.name}}
      <span class="arrow_wrap iconfont icon-right"></span> 
    </view>
  </navigator>
</view>
<view class="wode_out">退出</view>