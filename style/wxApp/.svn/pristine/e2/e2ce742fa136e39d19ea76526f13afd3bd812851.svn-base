.user-diary {
  background-color: rgb(255, 255, 255);
  width: 100%;
  border-bottom: 1rpx solid rgb(216, 216, 216);
  box-sizing: border-box;
  border-top: 1rpx solid rgb(216, 216, 216);
  margin-bottom:20rpx;
}
.active{
  background: #EDEDED;
}
.diary-info{
  padding: 0 20rpx;
}
.diary-img image{
	width:200rpx;
	margin:0 20rpx;
	height:200rpx;
}
.diary-all {
  width: 750rpx;
  font-size: 32rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: rgb(102, 102, 102);
}
.diary-but button{
  overflow:visible;
}
.diary-add{
	position:fixed;
	bottom:40rpx;
	right:40rpx;
	background:#F25A88;
	color:#fff;
	padding:30rpx;
	border-radius:50%;
	font-size:28rpx;
}
.weui-panel{
  margin: 20rpx 20rpx 0 !important;
  border: 1rpx solid #E5E5E5;
}
.quanpic{
	display:block;
	width:100%;
  border-radius:8rpx;
}
.weui-panel::after{
  border:none;
}
.weui-panel::before{
  border:none;
}
.weui-cell::before{
  left:0;
}