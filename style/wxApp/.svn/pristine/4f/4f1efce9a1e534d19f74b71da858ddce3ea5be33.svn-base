<view>
  <swiper class="banner" indicator-dots="{{navTop.indicatorDots}}" 
    autoplay="{{navTop.autoplay}}" interval="{{navTop.interval}}" duration="{{navTop.duration}}">
    <block wx:for="{{navTop.imgUrls}}">
      <swiper-item>
        <navigator url="../zhuanti/index?id={{item.src}}"><image src="{{item.src}}" class="slide-image" /></navigator>
      </swiper-item>
    </block>
  </swiper>
</view>
<view class="category">
  <view class="category_item" wx:for="{{iconItems.iconArray}}">
  <navigator url="../zhuanti/index?id={{item.id}}">
    <view class="category_item_wrap">
      <view class="icon_wrap">
        <image src="{{item.iconUrl}}" class="index_icon"></image>
      </view>
      <view class="category_item_text">
        <text>{{item.iconText}}</text>
      </view>
    </view>
    </navigator>
  </view>
</view>
