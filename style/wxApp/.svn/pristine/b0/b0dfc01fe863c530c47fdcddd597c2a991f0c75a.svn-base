/**app.wxss**/
@import '../weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  background: #f6f6fa;
  height: 100%;
  font-family: 微软雅黑;
}

/*常用样式*/
.fl{
  float: left;
}
.fr{
  float:right;
}
.text-right {
  text-align: right;
}

.text-center {
  text-align: center;
}

.bor-bot {
  border-bottom: 1rpx solid #d9d9d9;
}
.bor-top {
  border-top: 1rpx solid #d9d9d9;
}
.weight600 {
  font-weight: 600;
}

/*常用背景色*/

.bgf14 {
  background: #f14c7f;
}

.bgfff {
  background: #fff;
}
.bgf25{
  	background:#F25A88;
}
.bgccc{
  	background:#cccccc;
}

.btn-danger {
   background: #d9544f;
}
.btn-warning {
  background: #f1ac4d;
}
.btn-info {
  background: #5bc0de;
}
.btn-success {
  background: #5bb85d;
}
.btn-primary {
  background: #418bca;
}

/*字体大小*/

.font-14 {
  font-size: 28rpx;
}

.font-16 {
  font-size: 32rpx;
}

.font-18 {
  font-size: 36rpx;
}

/*字体颜色*/

.colfff {
  color: #fff;
}

.col333 {
  color: #333;
}

.col666 {
  color: #666;
}

.col999 {
  color: #999;
}

.colF25 {
    color: #F25A88;
}

/*常用边距*/

.ptb10 {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.plr10 {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.pd10{
  padding:20rpx;
}
.pd10{
  padding:20rpx;
}

.ptb5 {
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.mt10 {
  margin-top: 20rpx;
}

.mb20 {
  margin-bottom: 40rpx;
}

.mg10 {
  margin: 20rpx !important;
}

.mb10 {
  margin-bottom: 20rpx !important;
}
.pb10 {
  padding-bottom: 20rpx !important;
}
.pt10 {
  padding-top: 20rpx !important;
}
/*图标*/

@font-face {
  font-family: 'iconfont'; /* project id 232989 */
  src: url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.eot');
  src: url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.woff') format('woff'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.4rpx;
  -moz-osx-font-smoothing: grayscale;
}

.icon-integral:before {
  content: "\e897";
}

.icon-right:before {
  content: "\e65f";
}

.icon-report:before {
  content: "\e643";
}

.icon-team:before {
  content: "\e8c9";
}

.icon-cart:before {
  content: "\e887";
}

.icon-right {
  display: inline-block;
  vertical-align: -4rpx;
}
