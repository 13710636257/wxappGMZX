/**app.wxss**/
 @import 'style/weui.wxss'; /*在全局导入weui框架 */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  background: #f6f6fa;
  height: 100%;
  font-family: 微软雅黑;
}

/*常用样式*/

.fl {
  float: left;
}

.fr {
  float: right;
}

.text-right {
  text-align: right;
}

.text-center {
  text-align: center;
}

.bor-bot {
  border-bottom: 1rpx solid #d9d9d9;
}

.bor-top {
  border-top: 1rpx solid #d9d9d9;
}

.weight600 {
  font-weight: 600;
}

/*常用背景色*/

.bgf14 {
  background: #f14c7f;
}

.bgfff {
  background: #fff;
}

.bgf25 {
  background: #f25a88;
}

.bgccc {
  background: #ccc;
}

/*字体大小*/

.font-14 {
  font-size: 28rpx;
}

.font-16 {
  font-size: 32rpx;
}

.font-18 {
  font-size: 36rpx;
}

/*字体颜色*/

.colfff {
  color: #fff;
}

.col333 {
  color: #333;
}

.col666 {
  color: #666;
}

.col999 {
  color: #999;
}

.colF25 {
  color: #f25a88;
}

/*常用边距*/

.ptb10 {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.plr10 {
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.pd10 {
  padding: 20rpx;
}

.pd10 {
  padding: 20rpx;
}

.ptb5 {
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.mt10 {
  margin-top: 20rpx;
}

.mb20 {
  margin-bottom: 40rpx;
}

.mg10 {
  margin: 20rpx !important;
}

.mb10 {
  margin-bottom: 20rpx !important;
}

.pb10 {
  padding-bottom: 20rpx !important;
}

.pt10 {
  padding-top: 20rpx !important;
}

/*图标*/
/*字体图标*/
@font-face {
  font-family: 'iconfont';  /* project id 799876 */
  src: url('//at.alicdn.com/t/font_799876_0jn1tqxirxs.eot');
  src: url('//at.alicdn.com/t/font_799876_0jn1tqxirxs.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_799876_0jn1tqxirxs.woff') format('woff'),
  url('//at.alicdn.com/t/font_799876_0jn1tqxirxs.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_799876_0jn1tqxirxs.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:32px;
  color: #8b8b8b;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-caidan:before { content: "\e62f"; }

.icon-dingdan:before { content: "\e610"; }

.icon-jifen:before { content: "\e673"; }

.icon-dianhua:before { content: "\e621"; }

.icon-wodejifen:before { content: "\e616"; }

.icon-tuikuan:before { content: "\e66b"; }

.icon-sousuo:before { content: "\e60e"; }

.icon-youjiantou:before { content: "\e731"; }

.icon-dizhi:before { content: "\e606"; }

.icon-weishiyong:before { content: "\e641"; }

.icon-tubiaozhizuomoban:before { content: "\e607"; }

.icon-daifukuan:before { content: "\e612"; }

.icon-tuijian:before { content: "\e636"; }

.icon-Team:before { content: "\e61c"; }

.icon-right {
  display: inline-block;
  vertical-align: -4rpx;
}


.paging {
  width: 100%;
  height: 55rpx;
  background: #fff;
  font-size: 28rpx;
  border: 1px solid #e5e5e5;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.arrow {
  width: 100rpx;
  text-align: center;
  font-size: 36rpx;
}
.pageNumber {
  font-weight: 600;
}