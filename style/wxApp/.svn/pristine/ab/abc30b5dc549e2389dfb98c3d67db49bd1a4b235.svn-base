.personal_info {
  padding: 20px 0 0;
  background-color: #fff;
  border-bottom: 1rpx solid rgb(216, 216, 216);
  margin-bottom: 10px;
}

.photo_wrap {
  text-align: center;
}

.photo {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
}

.nickname {
  margin-top: 30rpx;
  text-align: center;
  color: #696969;
}

.wode_item_wrap {
  background-color: #fff;
  font-size: 28rpx;
  margin-top: 10px;
  border-top: 1px solid #e5e5e5;
}

.wode_item {
  height: 88rpx;
  border-bottom: 1px solid #e5e5e5;
  padding: 0 10px;
  line-height: 88rpx;
  position: relative;
  color: rgb(102, 102, 102);
}

.arrow_wrap {
  position: absolute;
  width: 50px;
  height: 88rpx;
  right: 0;
  top: 0;
  background-image: url(../../image/icon-arrow.png);
  background-repeat: no-repeat;
  background-size: 28rpx;
  background-position: 17px 14px;
}

.wode_out {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background-color: #fff;
  margin-top: 10px;
  font-size:28rpx;
  border-bottom: 1px solid #e5e5e5;
  border-top: 1px solid #e5e5e5;
}

/* 字体图标 */

@font-face {
  font-family: 'iconfont'; /* project id 232989 */
  src: url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.eot');
  src: url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.eot?#iefix') format('embedded-opentype'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.woff') format('woff'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_uvew9r99ucsdcxr.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.4rpx;
  -moz-osx-font-smoothing: grayscale;
}

.icon-integral:before {
  content: "\e897";
}
.icon-right:before {
  content: "\e65f";
}
.icon-report:before {
  content: "\e643";
}

.icon-team:before {
  content: "\e8c9";
}

.icon-cart:before {
  content: "\e887";
}

.user-article {
  background-color: rgb(255, 255, 255);
}

.user-order {
  display: flex;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid rgb(216, 216, 216);
  box-sizing: border-box;
  border-top: 1rpx solid rgb(216, 216, 216);
}

.order-all {
  width: 630rpx;
  font-size: 28rpx;
  color: rgb(102, 102, 102);
}

.order-look {
  font-size: 24rpx;
  color: rgb(153, 153, 153);
}

.icon-right {
  display: inline-block;
  vertical-align: -4rpx;
}

.order-class {
  display: flex;
  width: 100%;
  /*height: 186rpx;*/
  box-sizing: border-box;
}

.item-order {
  width: 25%;
  height: 155rpx;
  text-align: center;
  color: rgb(153, 153, 153);
}

.item-order .iconfont {
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-bottom: 10rpx;
  font-size: 80rpx;
}

.item--order-text {
  line-height: 34rpx;
  font-size: 24rpx;
}
